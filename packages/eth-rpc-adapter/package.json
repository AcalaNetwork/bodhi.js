{
  "name": "@acala-network/eth-rpc-adapter",
  "version": "2.6.5",
  "license": "MIT",
  "author": "Acala Developers <hello@acala.network>",
  "bin": "./bin/eth-rpc-adapter.js",
  "scripts": {
    "clean": "rm -rf lib tsconfig.tsbuildinfo",
    "build": "tsc --build tsconfig.json",
    "lint": "tsc --noEmit -p tsconfig.json && eslint \"**/*.ts\"",
    "test:e2e": "SKIP_PUBLIC=true mocha -r tsconfig-paths/register src/__tests__/e2e/*",
    "start": "ts-node -r tsconfig-paths/register src/index.ts",
    "dev": "nodemon --watch \"*\" --ext \"js,ts,json\" --ignore \"src/**/*.{spec,test}.ts\" --exec \"ts-node src/index.ts -l\" | pino-pretty",
    "test:CI": "mocha -r tsconfig-paths/register src/__tests__/e2e/* --timeout 300000",
    "ncc:pack": "ncc build src/index.ts -t --target es2020"
  },
  "dependencies": {
    "@acala-network/eth-providers": "workspace:*",
    "@acala-network/eth-transactions": "workspace:*",
    "@ethersproject/abstract-provider": "~5.7.0",
    "@ethersproject/abstract-signer": "~5.7.0",
    "@ethersproject/address": "~5.7.0",
    "@ethersproject/bignumber": "~5.7.0",
    "@ethersproject/bytes": "~5.7.0",
    "@ethersproject/logger": "~5.7.0",
    "@ethersproject/wallet": "~5.7.0",
    "@polkadot/api": "9.10.3",
    "axios": "~0.24.0",
    "body-parser": "~1.19.0",
    "connect": "~3.7.0",
    "cors": "~2.8.5",
    "dd-trace": "~2.6.0",
    "ethers": "~5.7.0",
    "pino": "~7.0.0-rc.3",
    "ws": "~8.2.2",
    "yargs": "16.2.0"
  },
  "devDependencies": {
    "@acala-network/contracts": "4.3.4",
    "@ethersproject/contracts": "~5.7.0",
    "@ethersproject/providers": "~5.7.0",
    "@polkadot/types": "8.1.1",
    "@types/body-parser": "~1.19.1",
    "@types/connect": "~3.4.35",
    "@types/cors": "~2.8.12",
    "@types/mocha": "~9.0.0",
    "@types/node": "~16.10.1",
    "@types/ws": "~8.2.0",
    "@types/yargs": "^16.0.4",
    "@vercel/ncc": "^0.36.1",
    "chai": "~4.3.4",
    "eslint": "~7.32.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "mocha": "~9.1.3",
    "nodemon": "~2.0.12",
    "pino-pretty": "~7.0.1",
    "ts-node": "~10.7.0",
    "typescript": "~4.6.3"
  },
  "files": [
    "lib",
    "bin"
  ],
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    },
    "./*": {
      "types": "./lib/*.d.ts",
      "default": "./lib/*.js"
    },
    "./package.json": "./package.json"
  }
}
