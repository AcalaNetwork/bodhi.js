    "@types/node-ipc": "^9.1.5",
    "@types/ws": "^8.2.0",
    "@rushstack/eslint-config": "^2.4.3",
    "eslint": "^7.32.0",
    "nodemon": "^2.0.12",
    "pino-pretty": "^7.0.1",
    "ts-node": "^10.3.0",
    "typescript": "^4.4.4",
    "@rushstack/heft": "^0.41.6",
    "mocha": "^9.1.3",
    "chai": "^4.3.4",
    "@types/chai-as-promised": "^7.1.4",
    "chai-as-promised": "^7.1.1"
  }
