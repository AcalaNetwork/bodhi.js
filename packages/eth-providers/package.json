{
  "name": "@acala-network/eth-providers",
  "version": "2.8.9",
  "license": "MIT",
  "author": "Acala Developers <hello@acala.network>",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf tsconfig.tsbuildinfo .nyc_output coverage/ lib/",
    "gql:typegen": "graphql-codegen --config codegen.yml",
    "test": "vitest",
    "test:coverage": "vitest --run --coverage"
  },
  "peerDependencies": {
    "@acala-network/api": "6.1.3",
    "@polkadot/api": "^12.4.2"
  },
  "dependencies": {
    "@acala-network/contracts": "4.3.4",
    "@acala-network/eth-transactions": "workspace:*",
    "bn.js": "~5.2.1",
    "ethers": "~5.7.0",
    "graphql": "~16.0.1",
    "graphql-request": "~3.6.1",
    "lru-cache": "~7.8.2"
  },
  "devDependencies": {
    "@acala-network/api": "6.1.3",
    "@polkadot/api": "^12.4.2",
    "@types/bn.js": "^5.1.5",
    "@types/lru-cache": "~7.6.1",
    "dotenv": "~10.0.0",
    "vitest": "^2.1.1"
  },
  "files": [
    "lib"
  ],
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    },
    "./*": {
      "types": "./lib/*.d.ts",
      "default": "./lib/*.js"
    },
    "./package.json": "./package.json"
  }
}
