{
  "name": "@acala-network/eth-rpc-adapter",
  "version": "2.4.20",
  "license": "MIT",
  "author": "Acala Developers <hello@acala.network>",
  "bin": "./bin/eth-rpc-adapter.js",
  "scripts": {
    "build": "rimraf dist && rollup -c",
    "lint": "tsc --noEmit && eslint \"**/*.ts\"",
    "test": "vitest __tests__",
    "test:dev": "vitest __tests__",
    "test:CI": "vitest --run __tests__",
    "start": "ts-node src/index",
    "dev": "nodemon --watch \"*\" --ext \"js,ts,json\" --ignore \"src/**/*.{spec,test}.ts\" --exec \"ts-node src/index.ts\" | pino-pretty"
  },
  "type": "module",
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "dependencies": {
    "@acala-network/eth-providers": "workspace:*",
    "@acala-network/eth-transactions": "workspace:*",
    "@ethersproject/abstract-provider": "~5.5.0",
    "@ethersproject/abstract-signer": "~5.5.0",
    "@ethersproject/address": "~5.5.0",
    "@ethersproject/bignumber": "~5.5.0",
    "@ethersproject/bytes": "~5.5.0",
    "@ethersproject/logger": "~5.5.0",
    "@ethersproject/wallet": "~5.5.0",
    "@polkadot/api": "8.1.1",
    "axios": "~0.24.0",
    "body-parser": "~1.19.0",
    "connect": "~3.7.0",
    "cors": "~2.8.5",
    "dotenv": "~10.0.0",
    "ethers": "~5.5.0",
    "node-ipc": "~10.1.0",
    "pino": "~7.0.0-rc.3",
    "ws": "~8.2.2",
    "dd-trace": "~2.6.0",
    "minimist": "~1.2.6",
    "@types/minimist": "~1.2.2"
  },
  "devDependencies": {
    "@rushstack/eslint-config": "~2.5.4",
    "@rushstack/heft": "~0.44.13",
    "@types/body-parser": "~1.19.1",
    "@types/chai": "~4.2.22",
    "@types/chai-as-promised": "~7.1.4",
    "@types/connect": "~3.4.35",
    "@types/cors": "~2.8.12",
    "@types/mocha": "~9.0.0",
    "@types/node": "~16.10.1",
    "@types/node-ipc": "~9.1.5",
    "@types/ws": "~8.2.0",
    "chai": "~4.3.4",
    "chai-as-promised": "~7.1.1",
    "eslint": "~7.32.0",
    "mocha": "~9.1.3",
    "nodemon": "~2.0.12",
    "pino-pretty": "~7.0.1",
    "ts-node": "~10.7.0",
    "typescript": "~4.6.3",
    "@polkadot/types": "8.1.1",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "rollup": "^2.77.0",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-node-externals": "^4.1.1",
    "rollup-plugin-typescript2": "~0.32.1",
    "tslib": "~2.4.0",
    "rimraf": "~3.0.2",
    "rollup-plugin-esbuild": "^4.9.1",
    "esbuild": "~0.14.49",
    "vitest": "~0.19.1"
  }
}
