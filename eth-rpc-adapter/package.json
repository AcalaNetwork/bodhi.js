  "bin": "./bin/eth-rpc-adapter.js",
  "scripts": {
    "build": "heft build --clean",
    "build:watch": "heft build",
    "watch": "rush build:watch --to @acala-network/eth-rpc-adapter",
    "lint": "tsc --noEmit && eslint \"**/*.ts\"",
    "test": "mocha **/*.test.ts --ignore **/e2e/**",
    "test:dev": "mocha **/*.test.ts --watch --watch-files **/*.test.ts",
    "test:CI": "mocha **/*.test.ts --timeout 300000",
    "start": "ts-node src/index",
    "dev": "nodemon --watch \"*\" --ext \"js,ts,json\" --ignore \"src/**/*.{spec,test}.ts\" --exec \"ts-node src/index.ts\" | pino-pretty"
  },
  "dependencies": {
    "@acala-network/eth-providers": "workspace:*",
    "@acala-network/eth-transactions": "workspace:*",
    "@acala-network/types": "^4.1.1",
